function printerError(s) {
    if (s==='') return 0
		var matched = /[n-z]/g; 
		var errors = s.match(matched) 
		if (errors === null || errors === undefined) {
			errors = '0'
		}
		else errors = errors.length.toString()
		var leng = s.length.toString()
  	if (!errors) {
			errors = '0'
		}
    else if (!leng) {
    leng = '0'
    }
	
